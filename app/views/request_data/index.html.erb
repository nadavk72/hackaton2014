<h1>Listing request_data</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Client</th>
      <th>Agent</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @request_data.each do |request_datum| %>
      <tr>
        <td><%= request_datum.date %></td>
        <td><%= request_datum.client_id %></td>
        <td><%= request_datum.agent_id %></td>
        <td><%= link_to 'Show', request_datum %></td>
        <td><%= link_to 'Edit', edit_request_datum_path(request_datum) %></td>
        <td><%= link_to 'Destroy', request_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

